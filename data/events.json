    "swap": {
        "toa2": {
            "micon": "trade.webp",
            "mdesc": "Trade between Unity and Technology",
            "removeRedundant": true,
            "showLimit": 3,
            "swapLimit": 2,
            "trades": [
                { "Exo-Toa": "Toa Mata Combination - Magma" },
                { "Exo-Toa": "Toa Mata Combination - Storm" },
                { "Exo-Toa": "Toa Mata Combination - Crystal" },
                { "The Boxor, Weapon of the Matoran": "Toa Mata Combination - Magma" },
